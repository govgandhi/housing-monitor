# Google Sheets CSV export URL (File > Share > Publish to web, or use /export?format=csv&gid=0)
SHEET_URL=https://docs.google.com/spreadsheets/d/YOUR_SHEET_ID/export?format=csv&gid=0

# Maximum monthly rent filter (listings at or above this are excluded)
MAX_RENT=3000

# Gmail credentials (use an App Password: https://myaccount.google.com/apppasswords)
GMAIL_USER=your.email@gmail.com
GMAIL_APP_PASSWORD=xxxx xxxx xxxx xxxx

# Comma-separated list of email recipients
RECIPIENT_EMAIL=your.email@gmail.com

# Send an email even when there are no new listings (true/false)
SEND_WHEN_NO_NEW=false
